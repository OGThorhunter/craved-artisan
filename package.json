{
  "name": "craved-artisan",
  "version": "1.0.0",
  "description": "A modern full-stack artisan marketplace",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev:mock": "cd server && npm run dev:mock",
    "dev:mock:simple": "cd server && tsx src/index-mock-simple.ts",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm run start",
    "start:client": "cd client && npm run start",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "cd server && npm run lint",
    "lint:client": "cd client && npm run lint",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm run test",
    "test:client": "cd client && npm run test",
    "db:generate": "cd server && npx prisma generate",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:seed": "tsx scripts/seed-vendor-dashboard.ts",
    "db:seed:settings": "tsx server/src/scripts/seed-settings-demo.ts",
    "db:studio": "cd server && npx prisma studio",
    "db:reset": "cd server && npx prisma migrate reset",
    "snapshot": "tsx scripts/projectSnapshot.ts > snapshot.json",
    "gen:api": "npm run gen:types && npm run gen:validators",
    "gen:types": "npx openapi-typescript openapi.yaml -o client/src/lib/api/types.ts",
    "gen:validators": "tsx scripts/generateValidators.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^6.0.0",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "cross-fetch": "^4.1.0",
    "express-rate-limit": "^7.3.1",
    "openapi-typescript": "^7.0.0",
    "openapi-zod-client": "^1.18.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "artisan",
    "marketplace",
    "handcrafted",
    "fullstack",
    "nextjs",
    "express",
    "prisma"
  ],
  "author": "Craved Artisan Team",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@prisma/instrumentation": "^6.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/json2csv": "^5.0.7",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.13",
    "json2csv": "^6.0.0-alpha.2",
    "multer": "^2.0.2",
    "openai": "^5.11.0",
    "pdfkit": "^0.17.1"
  }
}
